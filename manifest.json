{
  "manifest_version": 2,
  "name": "Chrome 插件开发模版",
  "version": "0.0.1",
  "description": "Chrome 插件开发模版",
  "content_scripts": [
    {
      "matches": ["*://www.baidu.com/*"],
      "js": ["src/inject.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/icon19.png",
      "38": "icons/icon38.png"
    },
    "default_title": "Chrome 插件开发模版",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": ["dist/app.js"]
}
